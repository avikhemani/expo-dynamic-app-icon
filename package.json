{
  "name": "kmoney-expo-dynamic-app-icon",
  "version": "1.0.2",
  "description": "Fork of expo-dynamic-app-icon with Android adaptive icon support",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "expo": {
    "plugin": "./plugin/build/withDynamicIcon.js"
  },
  "scripts": {
    "build": "tsc --build",
    "build:plugin": "tsc --project tsconfig.plugin.json",
    "prepublishOnly": "npm run build && npm run build:plugin"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-dynamic-app-icon",
    "adaptive-icon",
    "android",
    "ios",
    "config-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avikhemani/expo-dynamic-app-icon.git"
  },
  "bugs": {
    "url": "https://github.com/avikhemani/expo-dynamic-app-icon/issues"
  },
  "author": "Avi Khemani",
  "license": "MIT",
  "homepage": "https://github.com/avikhemani/expo-dynamic-app-icon#readme",
  "dependencies": {
    "@expo/config-plugins": "^9.0.0",
    "@expo/image-utils": "^0.4.0",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "expo": ">=49",
    "react": "*",
    "react-native": "*"
  },
  "files": [
    "build",
    "plugin",
    "src",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  }
